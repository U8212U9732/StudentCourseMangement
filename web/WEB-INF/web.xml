<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>UserLoginServlet</servlet-name>
        <servlet-class>servlet.UserLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClassFindAllServelt</servlet-name>
        <servlet-class>servlet.ClassFindAllServelt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClassDeleteServlet</servlet-name>
        <servlet-class>servlet.ClassDeleteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClassUpdateServlet</servlet-name>
        <servlet-class>servlet.ClassUpdateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClassAddServlet</servlet-name>
        <servlet-class>servlet.ClassAddServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClassFindByNameServlet</servlet-name>
        <servlet-class>servlet.ClassFindByNameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserClassAddServlet</servlet-name>
        <servlet-class>servlet.UserClassAddServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserClassFindAllServlet</servlet-name>
        <servlet-class>servlet.UserClassFindAllServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserClassFindAllServlet</servlet-name>
        <url-pattern>/UserClassFindAllServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserClassAddServlet</servlet-name>
        <url-pattern>/UserClassAddServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClassFindByNameServlet</servlet-name>
        <url-pattern>/ClassFindByNameServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClassAddServlet</servlet-name>
        <url-pattern>/ClassAddServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClassUpdateServlet</servlet-name>
        <url-pattern>/ClassUpdateServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClassDeleteServlet</servlet-name>
        <url-pattern>/ClassDeleteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClassFindAllServelt</servlet-name>
        <url-pattern>/ClassFindAllServelt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserLoginServlet</servlet-name>
        <url-pattern>/UserLoginServlet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
</web-app>